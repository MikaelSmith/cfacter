///
/// @file
/// Declares macros for the facter library version.
///
#ifndef FACTER_VERSION_H_
#define FACTER_VERSION_H_

///
/// The facter library major version.
///
#define LIBFACTER_VERSION_MAJOR @LIBFACTER_VERSION_MAJOR@
///
/// The facter library minor version.
///
#define LIBFACTER_VERSION_MINOR @LIBFACTER_VERSION_MINOR@
///
/// The facter library patch version.
///
#define LIBFACTER_VERSION_PATCH @LIBFACTER_VERSION_PATCH@

///
/// The facter library version as a string.
///
#define LIBFACTER_VERSION "@LIBFACTER_VERSION_MAJOR@.@LIBFACTER_VERSION_MINOR@.@LIBFACTER_VERSION_PATCH@"

#endif  // FACTER_VERSION_H_